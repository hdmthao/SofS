n                      Sign Out\n                    </Link>\n                  </li>\n                </ul>\n              </div>\n            ) : (\n              <Link className=\"size-cart border rounded-1 py-1\" to=\"/sign-in\">Sign In</Link>\n            )}\n            {userInfo && userInfo.type === 'seller' && (\n              <div className=\"dropdown\">\n                <Link to=\"#admin\">\n                  Seller <i className=\"fa fa-caret-down\"></i>\n                </Link>\n                <ul className=\"dropdown-content\">\n                  <li>\n                    <Link to=\"/productlist/seller\">Products</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/orderlist/seller\">Orders</Link>\n                  </li>\n                </ul>\n              </div>\n            )}\n            {userInfo && userInfo.type === 'admin' && (\n              <div className=\"dropdown\">\n                <Link to=\"#admin\">\n                  Admin <i className=\"fa fa-caret-down\"></i>\n                </Link>\n                <ul className=\"dropdown-content\">\n                  <li>\n                    <Link to=\"/dashboard\">Dashboard</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/productlist\">Products</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/orderlist\">Orders</Link>\n                  </li>\n                  <li>\n                    <Link to=\"/userlist\">Users</Link>\n                  </li>\n                </ul>\n              </div>\n            )}\n          </div>\n        </header>\n        <aside className={sidebarIsOpen ? 'open' : ''}>\n          <ul className=\"categories\">\n            <li>\n              <strong>Categories</strong>\n              <button\n                onClick={() => setSidebarIsOpen(false)}\n                className=\"close-sidebar\"\n                type=\"button\"\n              >\n                <i className=\"fa fa-close\"></i>\n              </button>\n            </li>\n            {loadingCategories ? (\n              <LoadingBox></LoadingBox>\n            ) : errorCategories ? (\n              <MessageBox variant=\"danger\">{errorCategories}</MessageBox>\n            ) : (\n              categories.map((c) => (\n                <li key={c.id}>\n                  <Link\n                    to={`/search/category/${c.id}`}\n                    onClick={() => setSidebarIsOpen(false)}\n                  >\n                    {c.name}\n                  </Link>\n                </li>\n              ))\n            )}\n          </ul>\n        </aside>\n        <main>\n          {/* <Route path=\"/seller/:id\" component={SellerScreen}></Route> */}\n          <Route path=\"/carts/:id?\" component={CartScreen}></Route>\n          <Route path=\"/products/:id\" component={ProductScreen} exact></Route>\n          {/* <Route\n            path=\"/product/:id/edit\"\n            component={ProductEditScreen}\n            exact\n          ></Route> */}\n          <Route path=\"/sign-in\" component={SignInScreen}></Route>\n          <Route path=\"/register\" component={RegisterScreen}></Route>\n          <Route path=\"/shippings\" component={ShippingAddressScreen}></Route>\n          <Route path=\"/payments\" component={PaymentMethodScreen}></Route>\n          <Route path=\"/place-orders\" component={PlaceOrderScreen}></Route>\n          {/* <Route path=\"/order/:id\" component={OrderScreen}></Route> */}\n          {/* <Route path=\"/orderhistory\" component={OrderHistoryScreen}></Route> */}\n          <Route\n            path=\"/search/name/:name?\"\n            component={SearchScreen}\n            exact\n          ></Route>\n          {/* <Route\n            path=\"/search/category/:category\"\n            component={SearchScreen}\n            exact\n          ></Route> */}\n          {/* <Route\n            path=\"/search/category/:category/name/:name\"\n            component={SearchScreen}\n            exact\n          ></Route> */}\n          {/* <Route\n            path=\"/search/category/:category/name/:name/min/:min/max/:max/rating/:rating/order/:order/pageNumber/:pageNumber\"\n            component={SearchScreen}\n            exact\n          ></Route> */}\n          {/* <PrivateRoute\n            path=\"/profile\"\n            component={ProfileScreen}\n          ></PrivateRoute> */}\n          <PrivateRoute path=\"/map\" component={MapScreen}></PrivateRoute>\n          {/* <AdminRoute\n            path=\"/productlist\"\n            component={ProductListScreen}\n            exact\n          ></AdminRoute>\n          <AdminRoute\n            path=\"/productlist/pageNumber/:pageNumber\"\n            component={ProductListScreen}\n            exact\n          ></AdminRoute>\n          <AdminRoute\n            path=\"/orderlist\"\n            component={OrderListScreen}\n            exact\n          ></AdminRoute>\n          <AdminRoute path=\"/userlist\" component={UserListScreen}></AdminRoute>\n          <AdminRoute\n            path=\"/user/:id/edit\"\n            component={UserEditScreen}\n          ></AdminRoute>\n          <SellerRoute\n            path=\"/productlist/seller\"\n            component={ProductListScreen}\n          ></SellerRoute>\n          <SellerRoute\n            path=\"/orderlist/seller\"\n            component={OrderListScreen}\n          ></SellerRoute> */}\n\n          <Route path=\"/\" component={HomeScreen} exact></Route>\n        </main>\n        <footer className=\"row center\">All right reserved</footer>\n      </div>\n    </BrowserRouter>\n  );\n}\n\nexport default App;\n","/Users/khoanguyen/Downloads/Projects/SofS/frontend/src/store.js",[],"/Users/khoanguyen/Downloads/Projects/SofS/frontend/src/serviceWorker.js",[],"/Users/khoanguyen/Downloads/Projects/SofS/frontend/src/components/SearchBox.js",[],"/Users/khoanguyen/Downloads/Projects/SofS/frontend/src/screens/CartScreen.js",